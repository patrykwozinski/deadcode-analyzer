{
  "type": "library",
  "name": "patrykwozinski/deadcode-analyzer",
  "description": "Tool to analyze your dead code and code usages in application",
  "license": "proprietary",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "localheinz/phpstan-rules": "^0.13.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.11.16",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpstan/phpstan-strict-rules": "^0.11.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "DeadCodeAnalyzer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DeadCodeAnalyzer\\Tests\\": "tests/"
    }
  }
}
